{
  "name": "ut-11ty-base",
  "version": "0.0.1",
  "description": "Starter template for 11ty builds",
  "main": "index.js",
  "scripts": {
    "build": "ELEVENTY_ENV=production eleventy",
    "build:clean": "npm run clean && npm run build",
    "build:debug": "npm run build --debug",
    "clean": "rimraf dist",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:css": "stylelint '**/*.pcss' --fix",
    "lint:js": "eslint \"_src/**/*.js\" --ignore-pattern 'node_modules/**/*' --fix",
    "serve": "concurrently \"npm:serve:*\"",
    "serve:11ty": "ELEVENTY_ENV=development eleventy --serve --quiet --incremental --port 7777",
    "test": "ava"
  },
  "engines": {
    "node": "16"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/inframanufaktur/ut-11ty-base.git"
  },
  "author": "ovl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inframanufaktur/ut-11ty-base/issues"
  },
  "homepage": "https://github.com/inframanufaktur/ut-11ty-base#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-vue": "^1.0.0-canary.8",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-alias": "^3.1.8",
    "autoprefixer": "^10.4.0",
    "ava": "^3.15.0",
    "axios": "^0.24.0",
    "concurrently": "^6.4.0",
    "csso": "^5.0.0",
    "dotenv": "^10.0.0",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eleventy-plugin-purgecss": "^0.2.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "markdown-it": "^12.3.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attribution": "^0.1.4",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-prism": "^2.2.2",
    "node-sass": "^7.0.0",
    "open-props": "^1.0.13",
    "postcss": "^8.4.4",
    "postcss-custom-properties": "^12.0.0",
    "postcss-import": "^14.0.2",
    "precss": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.45.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:js"
    ],
    "_src/**/*.(p)?css": [
      "npm run lint:css"
    ]
  }
}
